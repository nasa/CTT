    <screens>
        <description>Positive Outreach  - Pre-call: Tips to the Contact Tracer</description>
        <name>Tips_Screen</name>
        <label>Pre-call: Tips to the Contact Tracer</label>
        <locationX>952</locationX>
        <locationY>210</locationY>
        <allowBack>true</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>true</allowPause>
        <connector>
